{
  "env": { "es2021": true, "node": true, "commonjs": true },
  "extends": ["airbnb-base", "plugin:node/recommended", "prettier"],
  "plugins": ["import"],
  "parserOptions": { "ecmaVersion": 2021 },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "off",
    "consistent-return": "off"
  }
}
